
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>

    .mid_box {
        width: auto;
        margin: 0 auto;
        padding-bottom: 10px;
        border: solid 1px #045ea0;
        background: #fff;
        border-radius: 0 0 8px 8px;
        margin: 10px 0px 10px 10px;
    }

    .form_tab {
        margin: 0;
        position: relative;
        bottom: -15px;
        padding: 8px 30px 8px 15px !important;
        font-weight: bold;
        font-family: Arial !important;
        font-size: 9pt !important;
        width: 190px;
        color: white !important;
        background: #2d93f7;
        border-radius: 0px 15px 15px 0px;
    }

    .form_box_level {
        margin: 0 auto 10px auto;
        text-align: center;
    }
</style>
<div class="col-lg-12">
    <div id="banner-area" class="banner-area" style="background-image: url(images/banner/banner1.jpg)">
        <div class="banner-text">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="banner-heading">
                            <h1 class="border-title border-left">Registration </h1>
                            <ol class="breadcrumb">
                                <li> <a href="/">Home</a> </li>
                                <li>Registration</li>
                            </ol>
                        </div>
                    </div>
                    <!-- Col end -->
                </div>
                <!-- Row end -->
            </div>
            <!-- Container end -->
        </div>
        <!-- Banner text end -->
    </div>
    @await Html.PartialAsync("~/Views/Shared/_Registration.cshtml")
</div>
@section Scripts{
    <script type="text/javascript" src="~/Feature/Registration/Create-Registration.js"></script>
}
