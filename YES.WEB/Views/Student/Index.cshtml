@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
 
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="breadcrumb box-title">List Of Students</h3>
                    <ol class="breadcrumb pull-right">
                        <li><a href="~/Dashboard/Index"><i class="fa fa-dashboard"></i>&nbsp;Dashboard</a></li>
                        <li class="active">List Of Students</li>
                    </ol>
                </div>
                <hr />
                <div class="box-body">
                    <div id="example1_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                        <div class="row">
                            <div class="col-sm-12">
                                <table id="tblStudentList" class="table table-bordered table-striped dataTable table-responsive" role="grid" aria-describedby="example1_info">
                                    <thead>
                                        <tr role="row">
                                            <th>#</th>
                                            <th>App. No</th>
                                            <th>Reg No.</th>
                                            <th>Students Name</th>
                                            <th>Gender</th>
                                            <th>Mobile No</th>
                                            <th>Email ID</th>
                                            <th>Address</th>
                                            <th>Tools</th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
@section Scripts{
    <script type="text/javascript" src="~/Feature/Student/Student-List.js"></script>
}